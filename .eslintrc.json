{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "webix": true,
        "$$": true,
        "ymaps": true,
        "signalR": true,
        "scheduler": true,
        "APPNAME": true,
        "VERSION": true,
        "PRODUCTION": true,
        "BUILD_AS_MODULE": true,
        "node": true,
        "__dirname": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        // "indent": [
        //     "error",
        //     "tab",
        //     {"SwitchCase":1}
        // ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}